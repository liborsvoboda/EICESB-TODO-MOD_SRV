{
  "RootDirectory": "wwwroot/files/",
  "ExcludeDirectories": [
    "bin",
    "Properties"
  ],
  "TextFileExtensions": [
    "txt",
    "sln",
    "proj",
    "cs",
    "config",
    "asax",
    "css",
    "htm",
    "html",
    "xml",
    "js",
    "md"
  ],
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  }
}
